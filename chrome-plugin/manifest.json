{
  "manifest_version": 3,
  "name": "YouTube URL Extractor",
  "version": "1.0",
  "description": "Extract download links from YouTube videos",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "http://localhost:5000/",
    "http://127.0.0.1:5000/"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "icon.png"
  }
}